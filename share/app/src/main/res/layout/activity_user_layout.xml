<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:id="@+id/activity_main"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">


    <com.qin.tao.share.app.base.BaseTextView
        android:id="@+id/tipView"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/blue_10"
        android:gravity="center"
        android:text="你感兴趣的东西，可能在这里哦"
        android:textColor="@color/white"
        android:visibility="gone"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/rl_welfare"
                android:layout_width="match_parent"
                android:layout_height="@dimen/row_h_normal"
                android:background="@drawable/list_selected_item_bg"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/padding_normal">

                <ImageView
                    android:id="@+id/img_welfare"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/img_welfare"/>

                <com.qin.tao.share.app.base.BaseTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:layout_toRightOf="@+id/img_welfare"
                    android:text="@string/string_welfare"
                    android:textColor="@color/yc_black_75"
                    android:textSize="@dimen/font_normal"/>

                <ImageView
                    android:id="@+id/welfare_arrow"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/margin_min"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/public_buttom_arrow"
                    android:visibility="visible"/>

            </RelativeLayout>

            <View
                style="@style/SpaceLine"
                android:layout_marginLeft="@dimen/margin_small"
                android:layout_marginRight="@dimen/margin_small"/>

            <RelativeLayout
                android:id="@+id/rl_joke_text"
                android:layout_width="match_parent"
                android:layout_height="@dimen/row_h_normal"
                android:background="@drawable/list_selected_item_bg"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/padding_normal">

                <ImageView
                    android:id="@+id/img_joke_text"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/img_joke_text"/>

                <com.qin.tao.share.app.base.BaseTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:layout_toRightOf="@+id/img_joke_text"
                    android:text="@string/string_joke_text"
                    android:textColor="@color/yc_black_75"
                    android:textSize="@dimen/font_normal"/>

                <ImageView
                    android:id="@+id/joke_text_arrow"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/margin_min"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/public_buttom_arrow"
                    android:visibility="visible"/>

            </RelativeLayout>

            <View
                style="@style/SpaceLine"
                android:layout_marginLeft="@dimen/margin_small"
                android:layout_marginRight="@dimen/margin_small"/>

            <RelativeLayout
                android:id="@+id/rl_joke_img"
                android:layout_width="match_parent"
                android:layout_height="@dimen/row_h_normal"
                android:background="@drawable/list_selected_item_bg"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/padding_normal">

                <ImageView
                    android:id="@+id/img_joke_img"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/img_joke_img"/>

                <com.qin.tao.share.app.base.BaseTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:layout_toRightOf="@+id/img_joke_img"
                    android:text="@string/string_joke_img"
                    android:textColor="@color/yc_black_75"
                    android:textSize="@dimen/font_normal"/>

                <ImageView
                    android:id="@+id/joke_img_arrow"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/margin_min"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/public_buttom_arrow"
                    android:visibility="visible"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="15dp"
                android:background="@color/app_color_bg"/>

            <RelativeLayout
                android:id="@+id/rl_weChat"
                android:layout_width="match_parent"
                android:layout_height="@dimen/row_h_normal"
                android:background="@drawable/list_selected_item_bg"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/padding_normal">

                <ImageView
                    android:id="@+id/img_weChat"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/img_jingxuan"/>

                <com.qin.tao.share.app.base.BaseTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:layout_toRightOf="@+id/img_weChat"
                    android:text="@string/string_wechat"
                    android:textColor="@color/yc_black_75"
                    android:textSize="@dimen/font_normal"/>

                <ImageView
                    android:id="@+id/weChat_arrow"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/margin_min"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/public_buttom_arrow"
                    android:visibility="visible"/>

            </RelativeLayout>

            <View
                style="@style/SpaceLine"
                android:layout_marginLeft="@dimen/margin_small"
                android:layout_marginRight="@dimen/margin_small"/>

            <RelativeLayout
                android:id="@+id/rl_recommend"
                android:layout_width="match_parent"
                android:layout_height="@dimen/row_h_normal"
                android:background="@drawable/list_selected_item_bg"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/padding_normal">

                <ImageView
                    android:id="@+id/img_recommend"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/me_recommend_icon"/>

                <com.qin.tao.share.app.base.BaseTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:layout_toRightOf="@+id/img_recommend"
                    android:text="@string/string_recommend"
                    android:textColor="@color/yc_black_75"
                    android:textSize="@dimen/font_normal"/>

                <ImageView
                    android:id="@+id/recommend_arrow"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/margin_min"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/public_buttom_arrow"
                    android:visibility="visible"/>

            </RelativeLayout>

            <View
                style="@style/SpaceLine"
                android:layout_marginLeft="@dimen/margin_small"
                android:layout_marginRight="@dimen/margin_small"/>

            <RelativeLayout
                android:id="@+id/rl_collection"
                android:layout_width="match_parent"
                android:layout_height="@dimen/row_h_normal"
                android:background="@drawable/list_selected_item_bg"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/padding_normal">

                <ImageView
                    android:id="@+id/img_collection"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/me_recommend_icon"/>

                <com.qin.tao.share.app.base.BaseTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:layout_toRightOf="@+id/img_collection"
                    android:text="@string/string_collection"
                    android:textColor="@color/yc_black_75"
                    android:textSize="@dimen/font_normal"/>

                <ImageView
                    android:id="@+id/collection_arrow"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/margin_min"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/public_buttom_arrow"
                    android:visibility="visible"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="15dp"
                android:background="@color/app_color_bg"/>

            <RelativeLayout
                android:id="@+id/rl_reward"
                android:layout_width="match_parent"
                android:layout_height="@dimen/row_h_normal"
                android:background="@drawable/list_selected_item_bg"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/padding_normal">

                <ImageView
                    android:id="@+id/img_reward"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/img_reward"/>

                <com.qin.tao.share.app.base.BaseTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:layout_toRightOf="@+id/img_reward"
                    android:text="@string/string_reward"
                    android:textColor="@color/yc_black_75"
                    android:textSize="@dimen/font_normal"/>

                <ImageView
                    android:id="@+id/reward_arrow"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/margin_min"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/public_buttom_arrow"
                    android:visibility="visible"/>

            </RelativeLayout>


            <View
                android:id="@+id/adLine"
                style="@style/SpaceLine"
                android:layout_marginLeft="@dimen/margin_small"
                android:layout_marginRight="@dimen/margin_small"/>

            <RelativeLayout
                android:id="@+id/rl_about_me"
                android:layout_width="match_parent"
                android:layout_height="@dimen/row_h_normal"
                android:background="@drawable/list_selected_item_bg"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/padding_normal">

                <ImageView
                    android:id="@+id/img_about_me"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/more_about_us"/>

                <com.qin.tao.share.app.base.BaseTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_small"
                    android:layout_toRightOf="@+id/img_about_me"
                    android:text="@string/string_about_me"
                    android:textColor="@color/yc_black_75"
                    android:textSize="@dimen/font_normal"/>

                <ImageView
                    android:id="@+id/about_me_arrow"
                    android:layout_width="@dimen/icon_small"
                    android:layout_height="@dimen/icon_small"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/margin_min"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/public_buttom_arrow"
                    android:visibility="visible"/>

            </RelativeLayout>

            <View style="@style/SpaceLine"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
